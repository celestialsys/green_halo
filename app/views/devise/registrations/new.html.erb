<script type="text/javascript">
  $(document).ready(function(){
     $( "#new_temporary_user" ).submit(function( event ) {
      var ml = $("#temporary_user_email").val();
      var cml = $("#temporary_user_confirm_email").val();


      if(ml != cml){
        alert( "confirm email should match with email" );
        event.preventDefault();
      }
      if (ml == ""){
        alert( "please provide valid email" );
        event.preventDefault();
      }
      else if ($("#temporary_user_password").val() == ""){
        alert( "password should not be blank" );
        event.preventDefault();
      }
      else if($("#temporary_user_password_confirmation").val() == ""){
          alert( "password confirmation should not be blank" );
          event.preventDefault();
      }
      else if($("#temporary_user_first_name").val() == ""){
        alert( "first name should not be blank" );
        event.preventDefault();
      }
       else if($("#temporary_user_last_name").val() == ""){
        alert( "last name should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_position").val() == ""){
        alert( "position should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_company_name").val() == ""){
        alert( "company name should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_website").val() == ""){
        alert( "company website should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_address1").val() == ""){
        alert( "address1 should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_zipcode").val() == ""){
        alert( "zipcode should not be blank" );
        event.preventDefault();
      }

      else if($("#temporary_user_phone").val() == ""){
        alert( "phone should not be blank" );
        event.preventDefault();
      }

      


      });

     $("#temporary_user_zipcode").keypress(function (e) {
     //if the letter is not digit then display error and don't type anything
     if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
        //display error message
        $("#errmsg").html("Digits Only").show().css("color" ,"red");
               return false;
       }
      });

      $("#temporary_user_phone").keypress(function (e) {
     //if the letter is not digit then display error and don't type anything
     if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
        //display error message
        $("#errmsg1").html("Digits Only").show().css("color" ,"red");
               return false;
       }
      });

      $('#temporary_user_email').keyup(function() {
          $('span.error-keyup-7').remove();
          var inputVal = $(this).val();
          var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
          if(!emailReg.test(inputVal)) {
              $(this).after('<span class="error error-keyup-7">Invalid Email Format.</span>');
          }
      });

      $('#temporary_user_first_name').keyup(function() {
          $('span.error-keyup-7').remove();
          var inputVal = $(this).val();
          var nameReg = /^([a-zA-Z]+$)/;
          if(!nameReg.test(inputVal)) {
              $(this).after('<span class="error error-keyup-7">Alpabets only.</span>');
          }
      });



      

     
     
  });
</script>




